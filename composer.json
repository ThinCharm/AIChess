{
    "name": "thincharm/aichess-ml",
    "description": "A chess engine built on top of PHP and powered by Symfony.",
    "keywords": [],
    "require": {
        "php": ">=7.1",
        "php-ai/php-ml": "^0.7.0",
        "symfony/cache": "^4.2",
        "symfony/debug": "^4.2",
        "symfony/polyfill-apcu": "^1.10",
        "symfony/polyfill-php71": "^1.10",
        "symfony/polyfill-php72": "^1.10",
        "symfony/polyfill-php73": "^1.10",
        "symfony/property-access": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "AIChess\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
